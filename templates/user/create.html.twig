{% extends 'base.html.twig' %}
{% block title %}
	Fromulaire d'inscription
{% endblock %}
{% block body %}
	<h1>Formulaire d'inscription</h1>

	{{form_start(formView)}}
	{{form_errors(formView)}}

	<div class="row">
		<div class="col">
			{{form_row(formView.fullName)}}
			{{form_row(formView.email)}}
		</div>
		<div class="col">
			{{form_row(formView.password.first)}}
			{{form_row(formView.password.second)}}

		</div>
	</div>

	<button type="submit" class="btn btn-primary">
		<i class="fas fa-save"></i>
		Creer le l'utilisateur
	</button>
	{{form_end(formView)}}

{% endblock %}
