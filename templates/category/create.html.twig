{% extends "base.html.twig" %}

{% block title %}Creation de categorie
{% endblock %}

{% block body %}
	<h1>
		Creer une Categorie</h1>

	{{form_start(formView)}}
	{{form_errors(formView)}}

	<div class="row">
		<div class="col">
			{{form_row(formView.name)}}

		</div>
	</div>

	<button type="submit" class="btn btn-primary">
		<i class="fas fa-save"></i>
		Creer la categorie
	</button>
	{{form_end(formView)}}
{% endblock %}
